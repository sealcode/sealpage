{
	"compilerOptions": {
		"outDir": "./dist/",
		"moduleResolution": "node",
		"sourceMap": true,
		"module": "es6",
		"target": "es5",
		"jsx": "react",
		"allowJs": true,
		"strictNullChecks": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"allowSyntheticDefaultImports": true
	},
	"exclude": ["./node_modules/**/*"]
}

